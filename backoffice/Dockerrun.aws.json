{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "mysql",
      "image": "mysql:5.7",
      "environment": [
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "value": "hcmus-ebanking"
        },
        {
          "name": "MYSQL_DATABASE",
          "value": "ebanking_db"
        }        
      ],
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 3306,
          "containerPort": 3306
        }
      ]
    },
    {
      "name": "T2TrC-Bank",
      "image": "aktv4pro/backoffice:0.0.1-SNAPSHOT",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "links": [
        "mysql"
      ]
    }
  ]
}